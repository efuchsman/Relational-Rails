<%= form_with url: "/dealerships/#{@dealership.id}/cars", method: :post, local: true do |form| %>
<%= form.label "Make" %>
<%= form.text_field :make %>
<%= form.label "Model" %>
<%= form.text_field :model %>
<%= form.label "Year" %>
<%= form.text_field :year %>
<%= form.label "Miles" %>
<%= form.text_field :miles %>
<%= form.label "Available for lease?(true/false)" %>
<%= form.text_field :available_for_lease %>
<%= form.label "Price" %>
<%= form.text_field :price %>

<%= form.submit "Create Vehicle" %>
<% end %>
